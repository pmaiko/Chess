{
  "version": 2,
  "builds": [
    {
      "src": "server/dist/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/server/(.*)",
      "dest": "server/dist/main.js"
    }
  ],
  "installCommand": "npm install --server",
  "buildCommand": "npm run build",
  "devCommand": "npm run build",
  "outputDirectory": "server/dist",
  "public": true
}
