{
  "version": 2,
  "installCommand": "npm install",
  "buildCommand": "npm run build",
  "startCommand": "node server/dist/main.js",
  "builds": [
    {
      "src": "server/dist/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server/dist/main.js"
    }
  ]
}
