{
  "version": 2,
  "builds": [
    {
      "src": "*",
      "use": "@vercel/node"
    },
    {
      "src": "server/package.json",
      "use": "@vercel/node"
    },
    {
      "src": "server/dist/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/server/(.*)",
      "dest": "server/dist/main.js"
    }
  ]
}
