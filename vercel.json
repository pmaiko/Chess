{
  "version": 2,
  "outputDirectory": "build",
  "builds": [
    {
      "src": "build/server/main.js",
      "use": "@now/node"
    },
    {
      "src": "server/package.json",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "build/server/main.js"
    }
  ]
}
