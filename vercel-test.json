{
  "version": 2,
  "buildCommand": "npm run build",
  "builds": [
    {
      "src": "dist/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "server/dist/**/*",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/server/(.*)",
      "dest": "server/dist/main.js",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "HEAD", "OPTIONS"]
    },
    {
      "src": "/(.*)",
      "dest": "dist/$1"
    }
  ]
}
